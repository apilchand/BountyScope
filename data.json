        "icon": "ph-list-bullets",
        "tools": [
            {
                "name": "ParamSpider",
                "type": "cli",
                "command": "paramspider -d [TARGET] --exclude woff,css,png,svg,jpg -q -o $RECON_DIR/urls/paramspider",
